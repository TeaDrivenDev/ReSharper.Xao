<?xml version="1.0"?>
<package>
  <metadata>
    <id>ReSharper.Xao</id>
    <version>$version$</version>
    <authors>Igal Tabachnik</authors>
    <owners>Igal Tabachnik</owners>
    <title>Navigate between a XAML View and ViewModel.</title>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://raw.github.com/hmemcpy/ReSharper.Xao/master/license.txt</licenseUrl>
    <description>A plugin that allows you to quickly navigate between XAML Views/CodeBehind and the corresponding ViewModel files, based on naming convention. 
For example, navigating from CustomerDetailsView to CustomerDetailsViewModel and vice-versa, by pressing the Go to Related Files shortcut (Ctrl+Shift+Alt+G).</description>
    <projectUrl>https://github.com/hmemcpy/ReSharper.Xao</projectUrl>
    <iconUrl>https://raw.github.com/hmemcpy/ReSharper.Xao/master/logo.png</iconUrl>
    <copyright>Copyright &#x00A9; 2014 Igal Tabachnik</copyright>
    <tags>xaml viewmodel mvvm navigate view model</tags>
    <releaseNotes></releaseNotes>
    <dependencies>
      <dependency id="Resharper" version="[8.1,8.3)"/>
    </dependencies>
  </metadata>
  <files>
    <file src="..\output\$configuration$\8.1\*.*" target="ReSharper\v8.1\plugins"/>
    <file src="..\output\$configuration$\8.2\*.*" target="ReSharper\v8.2\plugins"/>
  </files>
</package>